{
  "license": "MIT",
  "version": "0.0.1",

  "name": "js-extensions",
  "description": "A set of extensions for common objects",
  "author": "patricio trevino <patricio@weirdpattern.com",

  "private": true,
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/weirdpattern/js-extensions.git"
  },

  "engines": {
    "node": ">=4.4.3"
  },

  "keywords": [
    "javascript",
    "extensions",
    "string",
    "int",
    "boolean",
    "date",
    "array",
    "object"
  ],

  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "coveralls": "^2.12.0",
    "prettier": "1.3.1",
    "jest": "^19.0.2",
    "jest-cli": "19.0.2",
    "webpack": "^2.2.1"
  },

  "scripts": {
    "build": "webpack --config .webpackrc",
    "test": "npm run pretty && npm run specs",
    "pretty": "prettier --print-width 120 --tab-width 2 --single-quote --trailing-comma all --write {lib,specs}/**/*.js",
    "specs": "jest --config .jestconfig",
    "coverage": "jest --config .jestconfig --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  }
}
